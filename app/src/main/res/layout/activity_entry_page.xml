<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/entryPage_RelativeLayout_baseLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/entry_background"
    android:paddingLeft="@dimen/activity_padding_left"
    android:paddingRight="@dimen/activity_padding_right"
    tools:context="com.v7idea.healthkit.EntryPage">

    <com.v7idea.template.View.AutoReleaseImageView
        android:id="@+id/entryPage_AutoReleaseImageView_ImageText"
        android:layout_width="300px"
        android:layout_height="60px"
        android:layout_centerInParent="true"
        android:scaleType="fitXY"
        android:src="@mipmap/entry_text" />

    <com.v7idea.template.View.AutoReleaseImageView
        android:id="@+id/entryPage_AutoReleaseImageView_middleLine"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/entryPage_AutoReleaseImageView_ImageText"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="34px"
        android:layout_marginLeft="65px"
        android:layout_marginRight="65px"
        android:scaleType="fitXY"
        android:src="@mipmap/entry_line" />

    <com.v7idea.template.View.AutoReleaseImageView
        android:id="@+id/entryPage_AutoReleaseImageView_Logo"
        android:layout_width="132px"
        android:layout_height="104px"
        android:layout_above="@+id/entryPage_AutoReleaseImageView_middleLine"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="34px"
        android:scaleType="fitXY"
        android:src="@mipmap/entry_logo" />

    <TextView
        android:id="@+id/entryPage_TextView_VersionName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/entryPage_AutoReleaseImageView_ImageText"
        android:layout_marginTop="10px"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="25px" />


</RelativeLayout>
